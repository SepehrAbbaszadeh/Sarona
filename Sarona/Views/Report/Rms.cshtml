@{
ViewData["Title"] = "RMS Weekly";
}

<h3>RMS Weekly</h3>
<form method="post" enctype="multipart/form-data" asp-controller="report" asp-action="rms">
    <div class="form-group">
        <div class="col-md-10">
            <div>Upload the RMS report (CSV file) from Abbasi website:</div>
            <input type="file" name="file" accept=".csv" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Upload" />
        </div>
    </div>
</form>



